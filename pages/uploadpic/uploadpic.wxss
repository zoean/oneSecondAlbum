  height:200rpx;
  overflow: hidden;
  font-size: 18px;
}
textarea{
	padding:0;
	margin:0;
}
.net-close{
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.7);
  position: fixed;
  top:0;
  left:0;
  z-index: 9999999;
}
.net-toast{
  width:540rpx;
